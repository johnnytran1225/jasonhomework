def name():
    print('     ')
    print('WHAT IS YOUR NAME ?')
    userinputname = ['JASON', 'jason']
    username=input()
    if username not in userinputname:
        print('THIS PROGRAM IS DESIGNED FOR JASON')
        print('     ')
        print('PLEASE ENTER THE CORRECT NAME')
        name()

    else:
        print('                             HELLO JASON WELCOME')

def age():
    print('     ')
    print('JASON, HOW OLD ARE YOU?')
    userage=input()
    userinputage = ['7']
    if userage not in userinputage:
        print('NO, THAT IS NOT YOUR AGE!')
        print('PLEASE ENTER CORRECT AGE.')
        age()
    else:
        print('YES JASON, YOU ARE 7 YEARS OLD ')





def question1():
    print('         WHAT IS 1 + 1 = ?')
    mathinput1=input('ENTER THE ANSWER: ')
    if ((mathinput1>='a' and mathinput1<='z') or (mathinput1>='A' and mathinput1<='Z')):
        print(mathinput1, 'IS NOT A NUMBER.\n')
        print('PLEASE ENTER A NUMBER')
        question1()
    elif int(mathinput1) > 2:
        print('     ')
        print('THAT IS TOO BIG')
        print('PLEASE TRY AGAIN')
        print('     ')
        question1()
    elif int(mathinput1) < 2:
        print('     ')
        print('THAT IS TOO SMALL')
        print('PLEASE TRY AGAIN')
        print('     ')
        question1()
    else:
        print('     ')
        print('YES, JASON THAT IS CORRECT')
        print('         1 + 1 = 2')
        print('LET GO TO THE NEXT QUESTION')
        print('     ')

