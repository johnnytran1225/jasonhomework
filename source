def question7():
    print('         WHAT IS 10 - 2 x 2 = ?')
    print('         REMINDER: MULTIPLY FIRST THEN SUBTRACT LATER ')
    mathinput7=input('ENTER THE ANSWER: ')
    if ((mathinput7>='a' and mathinput7<='z') or (mathinput7>='A' and mathinput7<='Z')):
        print(mathinput7, 'IS NOT A NUMBER.\n')
        print('PLEASE ENTER A NUMBER')
        question7()
    elif int(mathinput7) > 6:
        print('     ')
        print('THAT IS TOO BIG, COUNT AGAIN')
        print('     ')
        question7()
    elif int(mathinput7) < 6:
        print('     ')
        print('THAT IS TOO SMALL, COUNT AGAIN')
        print('     ')
        question7()
    else:
        print('     ')
        print('YES, JASON THAT IS CORRECT')
        print('         10 - (2 x 2) = 6')
        print('         FIRST MULTIPLY 2 x 2 = 4')
        print('         THEN SUBTRACT  10 - 4 = 6')
        print('LET GO TO THE NEXT QUESTION')
        print('     ')

def question8():
    print('        WHAT IS YOUR GRANDMA NAME?')
    print('        3 LETTERS')
    inputlist= ['BOT', 'bot']
    input1=input('PLEASE ENTER LETTERS HERE: ')
    if input1 not in inputlist:
        print('NO JASON, THAT IS NOT HER NAME')
        print('     ')
        print('TRY AGAIN')
        question8()
    else:
        print('     ')
        print('YES YOUR GRANDMA NAME IS: BOT')
        print('     ')

